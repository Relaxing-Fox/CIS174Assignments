@model CIS174Assignments.ViewModels.StudentsViewModel

<h1>Hi, Admin!</h1>
<hr />
<table class="table">
    <thead>
        <tr>
            <th>Firstname</th>
            <th>Lastname</th>
            <th>Grade</th>
        </tr>
        <tr></tr>
    </thead>
    <tbody>
        @foreach (var item in Model.StudentList)
        {
            <tr>
                <td>@item.FirstName</td>
                <td>@item.LastName</td>
                <td>@item.Grade</td>
            </tr>
        }
    </tbody>
</table>